b":{"base_uri":"https://localhost:8080/"},"id":"HM0tFC-o8DPy","executionInfo":{"status":"ok","timestamp":1695357597689,"user_tz":-330,"elapsed":8,"user":{"displayName":"HEMANTH SAI PAVAN IMMIDISETTY","userId":"08260291429688432063"}},"outputId":"22c3a44d-67cb-41e7-e1e8-05d0e4b221f9"},"outputs":[{"output_type":"stream","name":"stdout","text":["[1] \"Estimated Probability of Being Ruined: 0.5886\"\n"]}],"source":["# Set the seed for reproducibility\n","set.seed(123)\n","\n","# Parameters\n","initial_stake <- 2\n","target_amount <- 5\n","prob_win <- 0.5  # Probability of winning a round (fair game)\n","\n","# Number of simulations\n","num_simulations <- 10000\n","\n","# Simulate the gambler's ruin\n","simulate_gamblers_ruin <- function(initial_stake, target_amount, prob_win) {\n","  current_stake <- initial_stake\n","  while (current_stake > 0 && current_stake < target_amount) {\n","    if (runif(1) < prob_win) {\n","      current_stake <- current_stake + 1\n","    } else {\n","      current_stake <- current_stake - 1\n","    }\n","  }\n","  return(current_stake == 0)\n","}\n","\n","# Run simulations\n","num_ruined <- sum(replicate(num_simulations, simulate_gamblers_ruin(initial_stake, target_amount, prob_win)))\n","\n","# Calculate the probability of being ruined before winning\n","probability_ruined_before_winning <- num_ruined / num_simulations\n","\n","# Print the result\n","print(paste(\"Estimated Probability of Being Ruined:\", probability_ruined_before_winning))"]},{"cell_type":"code","source":["# Parameters\n","initial_stake <- 2\n","target_amount <- 5\n","prob_win <- 0.5  # Probability of winning a round (fair game)\n","\n","# Number of states in the Markov chain\n","num_states <- target_amount - initial_stake + 1\n","\n","# Initialize the transition matrix\n","transition_matrix <- matrix(0, nrow = num_states, ncol = num_states)\n","\n","# Fill in the transition probabilities\n","for (i in 1:num_states) {\n","  if (i == num_states) {\n","    transition_matrix[i, i] <- 1  # Terminal state\n","  } else {\n","    transition_matrix[i, i + 1] <- prob_win\n","    transition_matrix[i, i - 1] <- 1 - prob_win\n","  }\n","}\n","\n","# Print the transition matrix\n","print(transition_matrix)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"JtgPOXYOFHE4","executionInfo":{"status":"ok","timestamp":1695357597690,"user_tz":-330,"elapsed":8,"user":{"displayName":"HEMANTH SAI PAVAN IMMIDISETTY","userId":"08260291429688432063"}},"outputId":"301bd086-fd5c-48c8-d64f-f90af2c3f186"},"execution_count":31,"outputs":[{"output_type":"stream","name":"stdout","text":["     [,1] [,2] [,3] [,4]\n","[1,]  0.0  0.5  0.0  0.0\n","[2,]  0.5  0.0  0.5  0.0\n","[3,]  0.0  0.5  0.0  0.5\n","[4,]  0.0  0.0  0.0  1.0\n"]}]},{"cell_type":"code","source":["# Calculate the probability using matrix exponentiation\n","n <- 1000  # High matrix power\n","probability_ruined_before_winning = transition_matrix[1, num_states]\n","\n","for (i in 1:n) {\n","  transition_matrix <- transition_matrix %*% transition_matrix\n","  probability_ruined_before_winning = probability_ruined_before_winning + transition_matrix[1, num_states]\n","}\n","\n","# Print the estimated probability\n","print(paste(\"Estimated Probability of Being Ruined (Matrix Power):\", probability_ruined_before_winning))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"zVfPWuJdGtmB","executionInfo":{"status":"ok","timestamp":1695357597690,"user_tz":-330,"elapsed":7,"user":{"displayName":"HEMANTH SAI PAVAN IMMIDISETTY","userId":"08260291429688432063"}},"outputId":"4644ef1d-979d-408e-c682-3754f76747ad"},"execution_count":32,"outputs":[{"output_type":"stream","name":"stdout","text":["[1] \"Estimated Probability of Being Ruined (Matrix Power): 249.591789245489\"\n"]}]},{"cell_type":"code","source":["# Parameters\n","prob_win <- 0.5\n","initial_stake <- 2\n","target_amount <- 5\n","\n","# Calculate exact probability using formula\n","p_win <- prob_win\n","p_lose <- 1 - p_win\n","N <- initial_stake\n","M <- target_amount\n","\n","exact_probability_ruined_before_winning <- (1 - (p_win / p_lose)^N) / (1 - (p_win / p_lose)^(N + M))\n","\n","# Construct the transition matrix\n","num_states <- target_amount - initial_stake + 1\n","transition_matrix <- matrix(0, nrow = num_states, ncol = num_states)\n","\n","for (i in 1:num_states) {\n","  if (i == num_states) {\n","    transition_matrix[i, i] <- 1\n","  } else {\n","    transition_matrix[i, i + 1] <- prob_win\n","    transition_matrix[i, i - 1] <- 1 - prob_win\n","  }\n","}\n","\n","# Calculate the probability using matrix exponentiation\n","n <- 1000  # High matrix power\n","probability_ruined_before_winning <- transition_matrix[1, num_states]\n","\n","for (i in 1:n) {\n","  transition_matrix <- transition_matrix %*% transition_matrix\n","  probability_ruined_before_winning = probability_ruined_before_winning + transition_matrix[1, num_states]\n","}\n","\n","# Print the results\n","print(paste(\"Exact Probability of Being Ruined:\", exact_probability_ruined_before_winning))\n","print(paste(\"Estimated Probability of Being Ruined (Matrix Power):\", probability_ruined_before_winning))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"uoQqa-FFFIMI","executionInfo":{"status":"ok","timestamp":1695357597690,"user_tz":-330,"elapsed":7,"user":{"displayName":"HEMANTH SAI PAVAN IMMIDISETTY","userId":"08260291429688432063"}},"outputId":"4ff7edbe-479e-4836-f101-68236d389a95"},"execution_count":33,"outputs":[{"output_type":"stream","name":"stdout","text":["[1] \"Exact Probability of Being Ruined: NaN\"\n","[1] \"Estimated Probability of Being Ruined (Matrix Power): 249.591789245489\"\n"]}]}]}